{
  "name": "webpack-angular",
  "version": "1.0.0",
  "description": "ANGULAR/WEBPACK",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run watch-server\"",
    "dev": "better-npm-run dev",
    "watch-server": "better-npm-run watch-server"
  },
  "betterScripts": {
    "dev": {
      "command": "node_modules/.bin/webpack --debug --devtool source-map --progress --config webpack.hot.js && node hot.js",
      "env": {
        "PROCESS_PATH": "/public",
        "NODE_ENV": "development",
        "PORT": 3000,
        "HOST": "localhost"
      }
    },
    "watch-server": {
      "command": "node src/server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 9090,
        "HOST": "localhost"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kasselTrankos/angular-webpack.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kasselTrankos/angular-webpack/issues"
  },
  "homepage": "https://github.com/kasselTrankos/angular-webpack#readme",
  "dependencies": {
    "angular": "^1.4.8",
    "angular-animate": "^1.4.8",
    "angular-aria": "^1.4.8",
    "angular-material": "^1.0.1",
    "angular-ui-router": "^0.2.15",
    "express": "^4.13.3",
    "font-awesome": "^4.5.0",
    "webpack": "^1.12.10"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "better-npm-run": "0.0.5",
    "browser-sync": "^2.11.0",
    "clean-webpack-plugin": "^0.1.6",
    "concurrently": "^1.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "piping": "^0.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
